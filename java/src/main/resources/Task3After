[private, class, TextReversing, {, , , , , private, static, void, main(String[], args), throws, FileNotFoundException, {, , , , , , , , , File, file, =, new, File("src\\main\\resources\\TextBeforeReverse, txt");, , , , , , , , , Scanner, scanner, =, new, Scanner(file);, , , , , , , , , ArrayList<String>, text, =, new, ArrayList<>();, , , , , , , , , while, (scanner, hasNextLine()), {, , , , , , , , , , , , , text, add(scanner, nextLine());, , , , , , , , , }, , , , , , , , , , Collections, reverse(text);, , , , , , , , , PrintWriter, writer, =, new, PrintWriter("text, txt");, , , , , , , , , , for, (int, i, =, 0;, i, <, text, size();, i++), {, , , , , , , , , , , , , writer, println(text, get(i));, , , , , , , , , }, , , , , , , , , , writer, close();, , , , , , }, }, ]