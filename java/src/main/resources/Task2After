[PUBLIC, CLASS, TEXTREVERSING, {, , , , , PUBLIC, STATIC, VOID, MAIN(STRING[], ARGS), THROWS, FILENOTFOUNDEXCEPTION, {, , , , , , , , , FILE, FILE, =, NEW, FILE("SRC\\MAIN\\RESOURCES\\TEXTBEFOREREVERSE, TXT");, , , , , , , , , SCANNER, SCANNER, =, NEW, SCANNER(FILE);, , , , , , , , , ARRAYLIST<STRING>, TEXT, =, NEW, ARRAYLIST<>();, , , , , , , , , WHILE, (SCANNER, HASNEXTLINE()), {, , , , , , , , , , , , , TEXT, ADD(SCANNER, NEXTLINE());, , , , , , , , , }, , , , , , , , , , COLLECTIONS, REVERSE(TEXT);, , , , , , , , , PRINTWRITER, WRITER, =, NEW, PRINTWRITER("TEXT, TXT");, , , , , , , , , , FOR, (INT, i, =, 0;, i, <, TEXT, SIZE();, I++), {, , , , , , , , , , , , , WRITER, PRINTLN(TEXT, GET(I));, , , , , , , , , }, , , , , , , , , , WRITER, CLOSE();, , , , , , }, }, ]